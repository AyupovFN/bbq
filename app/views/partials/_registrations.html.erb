<% if user_signed_in? %>
  <li>
    <%= link_to('Edit registration', edit_user_registration_path) %>
  </li>
<% else %>
  <li>
    <%= link_to 'Зарегистрироваться', new_user_registration_path, :remote => true, 'data-toggle' => "modal",
                'data-target' => "#myModalregistration", method: 'get' %>
  </li>
<% end %>